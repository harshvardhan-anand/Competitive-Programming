    "def find_rev(a):\n",
    "    idx = len(a)-1\n",
    "    b = []\n",
    "    while idx>=0:\n",
    "        b.append(a[idx])\n",
    "        idx-=1\n",
    "    b = list(map(str, b))\n",
    "    print(' '.join(b))"
   ]
